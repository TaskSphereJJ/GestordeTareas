@model GestordeTaras.EN.Proyecto

@{
    ViewData["Title"] = "Detalles del Proyecto";
}

<div>

    <dl class="row">
        <dt class="col-sm-2">Título:</dt>
        <dd class="col-sm-10">@Model.Titulo</dd>
        
        <dt class="col-sm-2">Descripción:</dt>
        <dd class="col-sm-10">@Model.Descripcion</dd>
        
        <dt class="col-sm-2">Admin:</dt>
        <dd class="col-sm-10">@Model.Usuario.Nombre</dd>
        
        <dt class="col-sm-2">Fecha de Finalización:</dt>
        <dd class="col-sm-10">@Model.FechaFinalizacion</dd>
    </dl>
</div>
